{
  "version": 2,
  "builds": [
    {
      "src": "painel-rh/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/painel-rh/(.*)",
      "dest": "painel-rh/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/colaborador",
      "dest": "/colaborador.html"
    },
    {
      "src": "/trabalhe-conosco",
      "dest": "/trabalhe-conosco.html"
    },
    {
      "src": "/relatorio",
      "dest": "/relatorio.html"
    },
    {
      "src": "/portal-colaborador",
      "dest": "/portal-colaborador.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
